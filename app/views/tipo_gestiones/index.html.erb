<h1>Listing tipo_gestiones</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tipo_gestiones.each do |tipo_gestion| %>
  <tr>
    <td><%= tipo_gestion.nombre %></td>
    <td><%= link_to 'Show', tipo_gestion %></td>
    <td><%= link_to 'Edit', edit_tipo_gestion_path(tipo_gestion) %></td>
    <td><%= link_to 'Destroy', tipo_gestion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

